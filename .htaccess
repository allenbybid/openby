<IfModule mod_rewrite.c>
RewriteEngine on
RewriteRule index.html$ index.php?action=index
RewriteRule index/([a-z]*).html$ index.php?action=$1
RewriteBase /
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /index.php [L]
</IfModule>
<Limit GET POST>
order deny,allow
deny from all
allow from all
</Limit>
<Limit PUT DELETE>
order deny,allow
deny from all
</Limit>
